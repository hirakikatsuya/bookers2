<%= render partial: 'books/infonew',locals:{book:@book,user:current_user}%>
<h1>Users</h1>

<table>
  <tr>
    <th>images</th>
    <th>name</th>
    <th></th>
  </tr>
  <tr>
    <% @users.each do |user| %>
    <td><%= image_tag user.get_profile_image(80,80) %></td>
    <td><%= user.name %></td>
    <td><%= link_to 'Show',user_path(user.id) %></td>
  </tr>
</table>
<% end %>